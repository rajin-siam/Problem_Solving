#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,j,t;
    scanf("%d",&t);
    for(j=1;j<=t;j++)
    {
                scanf("%d",&n);
                printf("Case %d: ",j);
            for(i=1;i<=n;i++)
            {
                if(n%i==0)printf(" %d",i);
            }
            printf("\n");
            }
}
