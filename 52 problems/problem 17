#include<bits/stdc++.h>
using namespace std;

int main()
{
    int t,i,counter=0;
    scanf("%d",&t);
    cin.ignore();
    while(t--)
    {
        char s[100];
        counter=0;
        fgets(s,100,stdin);
        int l=strlen(s);
        for(i=0;i<l;i++){
            if(s[i]=='a'||s[i]=='e'||s[i]=='i'||s[i]=='o'||s[i]=='u'||s[i]=='A'||s[i]=='E'||s[i]=='I'||s[i]=='O'||s[i]=='U')
                counter++;
        }cout<<counter<<endl;

    }
}
