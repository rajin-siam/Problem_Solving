#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n,i,j,t,k;
    scanf("%d",&t);
    for(j=1;j<=t;j++)
    {
                scanf("%d",&n);
            for(i=1;i<=n;i++)
            {
                for(k=0;k<n;k++)printf("*");printf("\n");
            }
            printf("\n");
            }
}
